      <import feature="org.eclipse.wst.jsdt.feature" version="1.1.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.platform" version="4.4.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.jdt" version="3.5.0" match="compatible"/>
      <import feature="org.eclipse.jst.enterprise_ui.feature" version="3.1.0" match="compatible"/>
      <import feature="org.eclipse.wst.common_core.feature" version="3.1.0" match="compatible"/>
      <import feature="org.eclipse.jst.common.fproj.enablement.jdt" version="3.1.0" match="compatible"/>
