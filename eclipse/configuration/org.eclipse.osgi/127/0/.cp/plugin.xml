            class="com.sap.ndb.studio.bi.welcome.WelcomeHeaderFormEditor"
            default="false"
            icon="icons/program.gif"
            id="com.sap.ndb.studio.bi.welcomeEditor"
            name="%editor.name_XTIT">
      </editor>
   </extension>

