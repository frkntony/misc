    <extension
          point="com.sap.ndb.studio.quickView.sections">
       <Section
          id="com.sap.ndb.studio.quickview.Modeler"
             name="SAP HANA Modeler">
          <ActionLink
                class="com.sap.ndb.studio.bi.quickview.DeliveryUnitHyperlinkAdapter"
                description="%QUICK_VIEW_DELIVERY_UNITS_YDES"
                icon="icons/Delivery_Unit.gif"
                id="com.sap.ndb.studio.bi.quickview.deliveryunit"
                name="%QUICK_VIEW_DELIVERY_UNITS_XLNK">
          </ActionLink>
            <ActionLink
                  class="com.sap.ndb.studio.bi.quickview.actions.GenerateTimeDataHyperlinkAdapter"
                  description="%QUICK_VIEW_GENERATE_TIME_DATE_YDES"
                  icon="icons/calendar_GIF.gif"
                  id="com.sap.ndb.studio.modeler.quickview.generateTimeData"
                  name="%QUICK_VIEW_GENERATE_TIME_DATE_XLNK">
         </ActionLink>
            <ActionLink
                  class="com.sap.ndb.studio.bi.quickview.actions.ConfigureImportServerHyperlinkAdapter"
                  description="%QUICK_VIEW_CONFIGURE_IMPORT_SERVER_YDES"
                  icon="icons/Configure_Import_Server.png"
                  id="com.sap.ndb.studio.modeler.quickview.configureImportServer"
                  name="%QUICK_VIEW_CONFIGURE_IMPORT_SERVER_XLNK">
            </ActionLink>
       </Section>
