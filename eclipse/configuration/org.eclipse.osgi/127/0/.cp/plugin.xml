    <extension
          point="org.eclipse.ui.menus">
       <menuContribution
             allPopups="false"
             locationURI="popup:com.sap.ndb.studio.navigator.view?after=group.new">
          <menu
                icon="icons/modeling_perspective_icon_v2.png"
                id="com.sap.ndb.studio.navigator.sapsystemmenu.modeler"
                label="SAP HANA Modeler">
              <command
                   commandId="com.sap.ndb.studio.modeler.command.generatetimedata"
                   icon="icons/calendar_GIF.gif"
                   style="push">
             </command>
              <visibleWhen
                    checkEnabled="false">
                 <reference
                       definitionId="com.sap.ndb.studio.modeler.actions.sapsystemactions">
                 </reference>
              </visibleWhen>
          </menu>
       </menuContribution>
    </extension>
    <extension
          point="org.eclipse.ui.commands">
           <command
             id="com.sap.ndb.studio.modeler.command.generatetimedata"
             name="%GENERATE_TIME_DATA_XMIT">
       </command>
    </extension>
    <extension
          point="org.eclipse.ui.handlers">
       <handler
             class="com.sap.ndb.studio.bi.handlers.GenerateTimeDataHandler"
             commandId="com.sap.ndb.studio.modeler.command.generatetimedata">
          <enabledWhen>
             <reference
                   definitionId="com.sap.ndb.studio.navigator.definition.sapsystemselected">
             </reference>
          </enabledWhen>
       </handler>
